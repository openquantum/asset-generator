{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["prettier", "eslint:recommended"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "no-console": "off",
    "consistent-return": "off",
    "no-underscore-dangle": "off",
    "func-names": "off",
    "camelcase": "off"
  }
}
